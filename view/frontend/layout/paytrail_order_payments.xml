<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Paytrail\PaymentService\Block\Order\Payments" name="paytrail.order.payments" cacheable="false">
                <block class="Magento\Vault\Block\Customer\CreditCards" name="vault.cards.list" cacheable="false" template="Magento_Vault::cards_list.phtml">
                    <block class="Paytrail\PaymentService\Block\Customer\CardRenderer" name="paytrail.card.renderer" template="Magento_Vault::customer_account/credit_card.phtml"/>
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
