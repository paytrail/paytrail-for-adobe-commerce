{
    "name": "paytrail/paytrail-for-adobe-commerce",
    "description": "Paytrail is a payment gateway that offers 20+ payment methods for Finnish customers.",
    "authors": [
        {
            "name": "Solteq Oyj",
            "email": "magentoservices@solteq.com",
            "homepage": "https://github.com/Solteq",
            "role": "Developer"
        },
        {
            "name": "Joonas Loueranta",
            "email": "joonas.loueranta@paytrail.com",
            "homepage": "https://github.com/loueranta",
            "role": "Manager"
        }
    ],
    "require": {
        "php": "^8.0",
        "paytrail/paytrail-php-sdk": ">=2.7.3",
        "magento/framework": ">=102.0.0",
        "ext-curl": "*",
        "nesbot/carbon": "^2.57.0"
    },
    "require-dev": {
        "magento/magento-coding-standard": "*"
    },
    "conflict": {
        "markup/module-paytrail": "*"
    },
    "type": "magento2-module",
    "version": "2.2.1",
    "license": "MIT",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Paytrail\\PaymentService\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "keywords": [
        "magento 2",
        "adobe commerce",
        "payment gateway",
        "paytrail",
        "Recurring Payment"
    ],
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": false,
            "magento/magento-composer-installer": false,
            "magento/inventory-composer-installer": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
